{
    "version": 2,
    "name": "pos",
    "alias": "pos.jeffersonaguilar.now.sh",
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": { "cache-control": "s-maxage=31536000,immutable" },
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "headers": { "cache-control": "s-maxage=0" },
            "dest": "index.html"
        }
    ]
}