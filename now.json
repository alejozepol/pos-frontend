{
    "version": 2,
    "name": "pos-frontend",
    "alias": "dexpress.app",
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build"
        }
    ],
    "routes": [
        {"src": "^/assets/(.*)", "dest": "assets/$1"},
        {"src": "^/(.*)", "dest": "index.html"}
    ]
}
